	end
	
	Inter.Globals:Define("clock", ClockBuiltin)
	Inter.Globals:Define("print", PrintBuiltin)
	
	-- >> BUILTINS -----------------------------------------------------------------------------------------------------------

	-- << OVERRIDES ----------------------------------------------------------------------------------------------------------
	
	-- << IMPLEMENTS ------------------------------------------
	Inter.ExprVisitor = Expr.Visitor:new()
